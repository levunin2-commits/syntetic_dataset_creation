aker import Faker\n","from random import choice\n","\n","fake = Faker('ru_RU')\n","\n","def calculate_snils_control_number(snils_digits: str) -> str:\n","    if len(snils_digits) != 9:\n","        raise ValueError(\"Должно быть 9 цифр\")\n","    total = sum(int(snils_digits[i]) * (9 - i) for i in range(9))\n","    if total < 100:\n","        control = total\n","    elif total == 100 or total == 101:\n","        control = 0\n","    else:\n","        control = total % 101\n","        if control >= 100:\n","            control = 0\n","    return snils_digits + f\"{control:02d}\"\n","\n","def generate_valid_snils():\n","    while True:\n","        first9 = ''.join(str(fake.random_int(0, 9)) for _ in range(9))\n","        snils = calculate_snils_control_number(first9)\n","        if not snils.startswith('000'):\n","            return snils\n","\n","def overlay_data_on_template(template_path, output_path):\n","    # Генерация данных\n","    grades = choice(['С отличием', 'С отличием'])\n","    surname = fake.last_name().upper()\n","    name = fake.first_name().upper()\n","    patronymic = fake.middle_name().upper()\n","    full_name = f\"{surname} \\n {name} {patronymic}\"\n","\n","    registration_date = f'{fake.day_of_month()} {fake.month_name()} {fake.year()}'\n","\n","    year = fake.year()\n","\n","    school = f'муниципальное образовательное \\n учреждение средняя общеобразовательная \\n школа №{randint(1, 64)}'\n","\n","    number = str(randint(10**13, 10**14))\n","    number1 = ''\n","    for x in number:\n","      number1 = number1 + x + ' '\n","    number_display = f'{number1}'\n","\n","\n","    # Открываем шаблон\n","    img = Image.open(template_path).convert(\"RGB\")\n","    draw = ImageDraw.Draw(img)\n","\n","    # Шрифт (попробуем Times New Roman)\n","    font = ImageFont.truetype(\"/content/drive/MyDrive/datasets/Project Sirius/Times New Roman/timesnewromanpsmt.ttf\", 20)\n","    font1 = ImageFont.truetype(\"/content/drive/MyDrive/datasets/Project Sirius/Times New Roman/timesnewromanps_italicmt.ttf\", 20)\n","    small_font = ImageFont.truetype(\"/content/drive/MyDrive/datasets/Project Sirius/Times New Roman/timesnewromanpsmt.ttf\", 18)\n","\n","    # === КООРДИНАТЫ ПОЛЯ ===\n","    POS_NUMBER = (randint(100, 160), randint(430, 480))        # Номер СНИЛС\n","    POS_GRADES = (randint(130, 195), randint(390, 420))         # С отличием\n","    POS_FIO = (randint(440, 600), randint(70, 110))            # Ф.И.О.\n","    POS_year_PLACE = (596, 157)     # Дата и место рождения\n","    POS_school = (460, randint(170, 260))         # Пол\n","    POS_REG_DATE = (randint(125, 185), randint(495, 555))      # Дата регистрации\n","\n","    # Наносим текст\n","    draw.text(POS_NUMBER, number_display, fill=\"brown\", font=font)\n","    draw.text(POS_FIO, full_name, fill=\"black\", align='center', font=font)\n","    draw.text(POS_year_PLACE, year, fill=\"black\", font=small_font)\n","    draw.text(POS_school, school, fill=\"black\", align='center', font=font)\n","    draw.text(POS_REG_DATE, registration_date, fill=\"black\", font=font)\n","    draw.text(POS_GRADES, grades, fill=\"black\", font=font)\n","\n","    plt.imshow(img)\n","    plt.axis('off') # Отключает отображение осей координат\n","    plt.show()\n","\n","    # Сохраняем\n","    img.save(output_path)\n","    print(f\"✅ Заполненный СНИЛС сохранён: {output_path}\")\n","\n","    return {\n","        \"grades\": grades,\n","        \"number\": number,\n","        \"registration_date\": registration_date,\n","        \"full_name\": full_name,\n","        \"year\": year,\n","        \"school\": school,\n","    }\n","\n","# Запуск\n","if __name__ == \"__main__\":\n","  for i in range(100):\n","      template = \"/content/drive/MyDrive/datasets/Project Sirius/2025-10-24_16-04-19.png\"\n","      output = f\"/content/drive/MyDrive/datasets/Project Sirius/attestat/document_{i}.jpg\"\n","\n","      if not os.path.exists(template):\n","          print(f\"❌ Шаблон не найден: {templat